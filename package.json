{
  "name": "@quallaa/cli",
  "version": "1.0.1",
  "description": "Sets up core infrastructure services for AI-assisted development",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "quallaa": "./dist/bin/quallaa.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build && npm run test:run",
    "start": "node dist/bin/quallaa.js"
  },
  "keywords": [
    "cli",
    "quallaa",
    "ai",
    "development",
    "automation",
    "domain-engineering"
  ],
  "author": "Quallaa",
  "license": "MIT",
  "homepage": "https://github.com/quallaa/quallaa-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quallaa/quallaa-cli.git"
  },
  "bugs": {
    "url": "https://github.com/quallaa/quallaa-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "inquirer": "^8.2.6",
    "keytar": "^7.9.0",
    "ora": "^5.4.1",
    "update-notifier": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.11.24",
    "@types/update-notifier": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
