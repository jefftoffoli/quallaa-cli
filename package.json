{
  "name": "quallaa-cli",
  "version": "1.2.2",
  "description": "Domain-driven infrastructure automation with 5 outcome pack templates for professional services, nonprofits, e-commerce, CRM, and financial operations",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "quallaa": "./dist/bin/quallaa.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "start": "node dist/bin/quallaa.js"
  },
  "keywords": [
    "cli",
    "quallaa",
    "ai",
    "development",
    "automation",
    "domain-engineering",
    "outcome-templates",
    "business-automation",
    "professional-services",
    "nonprofit",
    "e-commerce",
    "crm",
    "financial-operations",
    "billing",
    "invoicing",
    "donation-management",
    "inventory-management",
    "lead-management",
    "revenue-recognition",
    "quickbooks",
    "hubspot",
    "stripe",
    "shopify"
  ],
  "author": "Quallaa",
  "license": "MIT",
  "homepage": "https://github.com/jefftoffoli/quallaa-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefftoffoli/quallaa-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jefftoffoli/quallaa-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "glob": "^11.0.3",
    "inquirer": "^8.2.6",
    "keytar": "^7.9.0",
    "ora": "^5.4.1",
    "semver": "^7.7.2",
    "update-notifier": "^5.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.11.24",
    "@types/semver": "^7.7.0",
    "@types/update-notifier": "^5.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
